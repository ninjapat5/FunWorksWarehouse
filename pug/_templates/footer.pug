mixin footer
	footer.v-align-parent
		.fluid-container.v-align-child
			.row
				.col-md-4
					.copyright
						p Â© Copyright 2017 FunWorks Warehouse
				.col-md-4
					.footer-contact
						.row
							.col-md-6
								p.phone
									i.fa.fa-phone.fa-3x(aria-hidden="true")
									a(href="tel:6577141234") (657) 714-1234
							.col-md-6
								p.email
									i.fa.fa-envelope.fa-3x(aria-hidden="true")
									a(href="mailto:info@funworkswarehouse.com") info@funworkswarehouse.com
						.row
							.col-md-12
								p.address
									i.fa.fa-map-marker.fa-3x(aria-hidden="true")
									a(href="http://bit.ly/funworkswarehouselocation" target="_blank") 19702-19710 Coastline Lane <br> Huntington Beach, CA 92648
				.col-md-4
					.social-media
						ul
							li.facebook
								a(href="https://www.facebook.com/funworkswarehouse" target="_blank")
									i.fa.fa-facebook-square.fa-4x(aria-hidden="true")
							li.twitter
								a(href="https://twitter.com/FwWarehouse" target="_blank")
									i.fa.fa-twitter-square.fa-4x(aria-hidden="true")
							li.instagram
								a(href="https://www.instagram.com/funworks_warehouse/" target="_blank")
									i.fa.fa-instagram.fa-4x(aria-hidden="true")
							li.google-plus
								a(href="https://plus.google.com/117773947179482044115/about" target="_blank")
									i.fa.fa-google-plus-square.fa-4x(aria-hidden="true")
							li.pinterest
								a(href="https://www.pinterest.com/funworkswarehou/" target="_blank")
									i.fa.fa-pinterest-square.fa-4x(aria-hidden="true")